for i, ide in enumerate(contatos_df['ID']):
    driver.find_element_by_xpath('//*[@id="matriculaSearch-selectized"]').send_keys(ide) #Buscar ID
    time.sleep(1)
    driver.find_element_by_xpath('//*[@id="matriculaSearch-selectized"]').send_keys(Keys.ENTER) #Selecionar ID
    time.sleep(1)
    driver.find_element_by_xpath('/html/body/main/div/div/div/div[1]/div/a[1]').click() #Clicar em Gestão Acadêmica
    time.sleep(1)
    driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr/td[7]/div/button').click() #clicar em ações
    time.sleep(1)
    driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr/td[7]/div/div/a').click() #clicar em boletim
    time.sleep(1)
# AV Primeira disciplina
    try:
        disciplina = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4]').text 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4][text()!="Ambientação"]') 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[7][text()<"70"]') 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/button').click() 
        time.sleep(1)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/div/a[2]').click() 
        time.sleep(1)
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[3][text()!="Concluido"]')
            if av in cronograma:
                c1()
        except:
            print(disciplina,"sem ação")
            time.sleep(1)
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[2]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[2]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c2()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[3]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[3]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c3()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[4]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[4]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c4()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[5]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[5]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c5()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[6]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[6]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c6()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[7]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[7]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c7()   
        except:
            print(disciplina,"sem ação")
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[8]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[8]/td[3][text()!="Concluido"]')        
            if av in cronograma:
                c8()   
        except:
            print(disciplina,"sem ação")
    except:
        print(disciplina, "está aprovada", file=arquivo)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/button/i').click() 
        time.sleep(1)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/div/a[2]').click() 
        time.sleep(1)
# AP Primeira disciplina
    try:
        driver.find_element_by_xpath('/html/body/main/div/div/h5/a').click()
        disciplina = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4]').text 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4][text()!="Ambientação"]') 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[7][text()<"70"]') 
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/button').click() 
        time.sleep(1)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[9]/div/div/a[2]').click() 
        time.sleep(1)
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[3][text()!="Concluido"]')
            if ap in cronograma:
                c1()
        except:
            print(disciplina,"sem ação")
            time.sleep(1)
        try:
            cronograma = driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[2]/td[4]').text 
            driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[2]/td[3][text()!="Concluido"]')        
            if ap in cronograma:
                c2()   
        except:
            print(disciplina,"sem ação")
