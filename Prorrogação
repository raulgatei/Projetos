import pandas as pd
import time
from selenium import webdriver
from selenium.webdriver.common import keys
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.chrome.options import Options  


arquivo = open('Prorro.txt', 'w')
contatos_df = pd.read_excel("Prorrogação.xlsx")
login = input(str('Insirar seu login: '))
senha = input(str('Insira sua senha: '))
av = "[Avaliação Virtual] 02"
ap = "2ª"

# chrome_options = Options()
# chrome_options.add_argument("--headless") 
# options = chrome_options
driver = webdriver.Chrome()#chrome_options=options)
driver.maximize_window()
driver.get("https://kroton.platosedu.io/erp/login/auth")
driver.find_element_by_id('usuario').send_keys(login)
driver.find_element_by_id('password').send_keys(senha + Keys.ENTER)
time.sleep(1)

def c1():
    try:
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[11]/div/button').click() 
        time.sleep(1)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[2]/div/table/tbody/tr[1]/td[11]/div/div/a[1]').click() 
        time.sleep(1)
        driver.find_element_by_xpath('//*[@id="dataFim"]').click()
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys(Keys.BACKSPACE) #Apagar dados
        driver.find_element_by_xpath('//*[@id="dataFim"]').send_keys("17/12/2021" + Keys.ENTER)
        time.sleep(1)
        print(ide,"|", disciplina,"|", cronograma,"|", "prorrogado", file=arquivo)
        driver.find_element_by_xpath('/html/body/main/div/div/div/div[1]/a').click()
        time.sleep(1)
    except:
        print(cronograma, "nenhuma alteração feita")
